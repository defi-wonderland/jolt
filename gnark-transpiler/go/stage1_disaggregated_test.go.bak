// TestDisaggregatedClaims tests each intermediate value
func TestDisaggregatedClaims(t *testing.T) {
	assert := test.NewAssert(t)

	// Load expected values from JSON (generated by Rust)
	expected := loadExpectedValues(t)
	t.Logf("Loaded expected values from testdata/expected_values.json")

	// Use same witness data as TestStage1Circuit - copy the assignment
	baseAssignment := buildStage1AssignmentForDisaggregated(t)
	t.Logf("Loaded witness data from testdata/witness_data.json")

	// Create disaggregated assignment with expected intermediate values from Rust
	assignment := &DisaggregatedCircuit{
		Stage1Circuit: *baseAssignment,
	}

	// Expected r0 from Rust JSON
	assignment.ExpectedR0, _ = new(big.Int).SetString(expected.R0, 10)

	// Expected claim_after_uni_skip from Rust JSON
	assignment.ExpectedClaimAfterUniSkip, _ = new(big.Int).SetString(expected.ClaimAfterUniSkip, 10)

	// Expected challenges from Rust JSON
	for i, s := range expected.Challenges {
		assignment.ExpectedChallenges[i], _ = new(big.Int).SetString(s, 10)
	}

	// Expected claims after each round from Rust JSON
	for i, s := range expected.ClaimsAfterRound {
		assignment.ExpectedClaimsAfterRound[i], _ = new(big.Int).SetString(s, 10)
	}

	// Test
	t.Log("Testing disaggregated circuit with explicit intermediate assertions...")
	t.Logf("Checking: r0=%s..., claim_after_uni_skip=%s...", expected.R0[:20], expected.ClaimAfterUniSkip[:20])
	t.Logf("Checking: %d challenges, %d claims", len(expected.Challenges), len(expected.ClaimsAfterRound))

	var circuit DisaggregatedCircuit
	err := test.IsSolved(&circuit, assignment, ecc.BN254.ScalarField())
	assert.NoError(err, "Disaggregated circuit should be satisfied")

	// Compile to verify
	cs, err := frontend.Compile(ecc.BN254.ScalarField(), r1cs.NewBuilder, &circuit)
	assert.NoError(err, "Circuit should compile")
	t.Logf("Disaggregated circuit constraints: %d", cs.GetNbConstraints())

	t.Log("SUCCESS: All intermediate values verified against Rust JSON:")
	t.Log("  - r0: MATCH")
	t.Log("  - claim_after_uni_skip: MATCH")
	t.Logf("  - challenges[0-%d]: MATCH", len(expected.Challenges)-1)
	t.Logf("  - claims_after_round[0-%d]: MATCH", len(expected.ClaimsAfterRound)-1)
}

// buildStage1AssignmentForDisaggregated loads witness data from JSON and returns Stage1Circuit assignment
func buildStage1AssignmentForDisaggregated(t *testing.T) *Stage1Circuit {
	witness := loadWitnessData(t)
	expected := loadExpectedValues(t)
	assignment := &Stage1Circuit{}

	// Preamble
	assignment.PreambleMaxInputSize = witness.Preamble.MaxInputSize
	assignment.PreambleMaxOutputSize = witness.Preamble.MaxOutputSize
	assignment.PreambleMemorySize = witness.Preamble.MemorySize
	assignment.PreambleInputChunk0, _ = new(big.Int).SetString(witness.Preamble.InputChunk0, 10)
	assignment.PreambleOutputChunk0, _ = new(big.Int).SetString(witness.Preamble.OutputChunk0, 10)
	assignment.PreamblePanic = witness.Preamble.Panic
	assignment.PreambleRamK = witness.Preamble.RamK
	assignment.PreambleTraceLength = witness.Preamble.TraceLength

	// Commitments (41 commitments Ã— 12 chunks each)
	setCommitmentChunks(assignment, witness.Commitments)

	// UniSkip coefficients (28)
	setUniSkipCoeffs(assignment, witness.UniSkipCoeffs)

	// Sumcheck polynomials (11 rounds Ã— 3 coefficients)
	setSumcheckPolys(assignment, witness.SumcheckPolys)

	// ExpectedFinalClaim from expected values JSON
	assignment.ExpectedFinalClaim, _ = new(big.Int).SetString(expected.FinalClaim, 10)

	return assignment
}

