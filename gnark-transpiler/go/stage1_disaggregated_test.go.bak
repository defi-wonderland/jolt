// ExpectedValues represents the expected intermediate values from Rust
type ExpectedValues struct {
	R0                string   `json:"r0"`
	ClaimAfterUniSkip string   `json:"claim_after_uni_skip"`
	Challenges        []string `json:"challenges"`
	ClaimsAfterRound  []string `json:"claims_after_round"`
	FinalClaim        string   `json:"final_claim"`
}

// WitnessData represents witness inputs from Rust
type WitnessData struct {
	Preamble      PreambleData   `json:"preamble"`
	Commitments   [][]string     `json:"commitments"`
	UniSkipCoeffs []string       `json:"uni_skip_coeffs"`
	SumcheckPolys [][]string     `json:"sumcheck_polys"`
}

type PreambleData struct {
	MaxInputSize  uint64 `json:"max_input_size"`
	MaxOutputSize uint64 `json:"max_output_size"`
	MemorySize    uint64 `json:"memory_size"`
	InputChunk0   string `json:"input_chunk0"`
	OutputChunk0  string `json:"output_chunk0"`
	Panic         uint64 `json:"panic"`
	RamK          uint64 `json:"ram_k"`
	TraceLength   uint64 `json:"trace_length"`
}

// loadExpectedValues loads expected values from JSON file generated by Rust
func loadExpectedValues(t *testing.T) *ExpectedValues {
	data, err := os.ReadFile("testdata/expected_values.json")
	if err != nil {
		t.Fatalf("Failed to read expected_values.json: %v", err)
	}
	var expected ExpectedValues
	if err := json.Unmarshal(data, &expected); err != nil {
		t.Fatalf("Failed to parse expected_values.json: %v", err)
	}
	return &expected
}

// loadWitnessData loads witness data from JSON file generated by Rust
func loadWitnessData(t *testing.T) *WitnessData {
	data, err := os.ReadFile("testdata/witness_data.json")
	if err != nil {
		t.Fatalf("Failed to read witness_data.json: %v", err)
	}
	var witness WitnessData
	if err := json.Unmarshal(data, &witness); err != nil {
		t.Fatalf("Failed to parse witness_data.json: %v", err)
	}
	return &witness
}

// DisaggregatedCircuit tests each intermediate value individually
// It reuses the same structure as Stage1Circuit but adds assertions for each step
type DisaggregatedCircuit struct {
	// Same inputs as Stage1Circuit
	Stage1Circuit

	// Expected intermediate values from Rust (all public for verification)
	ExpectedR0                frontend.Variable   `gnark:",public"`
	ExpectedClaimAfterUniSkip frontend.Variable   `gnark:",public"`
	ExpectedChallenges        [11]frontend.Variable `gnark:",public"`
	ExpectedClaimsAfterRound  [11]frontend.Variable `gnark:",public"`
}

func (circuit *DisaggregatedCircuit) Define(api frontend.API) error {
	// === BUILD TRANSCRIPT (same as Stage1Circuit) ===

	// Start with "Jolt" label
	state := poseidon.Hash(api, 1953263434, 0, 0)

	// Append preamble (indices 0-7)
	state = poseidon.Hash(api, state, 0, poseidon.ByteReverse(api, circuit.PreambleMaxInputSize))
	state = poseidon.Hash(api, state, 1, poseidon.ByteReverse(api, circuit.PreambleMaxOutputSize))
	state = poseidon.Hash(api, state, 2, poseidon.ByteReverse(api, circuit.PreambleMemorySize))
	state = poseidon.Hash(api, state, 3, poseidon.ByteReverse(api, circuit.PreambleInputChunk0))
	state = poseidon.Hash(api, state, 4, poseidon.ByteReverse(api, circuit.PreambleOutputChunk0))
	state = poseidon.Hash(api, state, 5, poseidon.ByteReverse(api, circuit.PreamblePanic))
	state = poseidon.Hash(api, state, 6, poseidon.ByteReverse(api, circuit.PreambleRamK))
	state = poseidon.Hash(api, state, 7, poseidon.ByteReverse(api, circuit.PreambleTraceLength))

	// Append all 41 commitments (indices 8-499)
	idx := 8
	state = appendCommitments(api, state, &idx, circuit)

	// Derive 11 tau challenges (indices 500-510)
	for i := 0; i < 11; i++ {
		state = poseidon.Hash(api, state, idx, 0)
		idx++
	}

	// Append 28 uni_skip coefficients (indices 511-538)
	state = appendUniSkipCoeffs(api, state, &idx, circuit)

	// Derive r0 (index 539)
	state = poseidon.Hash(api, state, idx, 0)
	idx++
	r0 := poseidon.Truncate128Reverse(api, state)

	// === CHECK 1: r0 ===
	api.AssertIsEqual(r0, circuit.ExpectedR0)

	// === COMPUTE claim_after_uni_skip ===
	claimAfterUniSkip := evaluateUniSkipPoly(api, circuit, r0)

	// === CHECK 2: claim_after_uni_skip ===
	api.AssertIsEqual(claimAfterUniSkip, circuit.ExpectedClaimAfterUniSkip)

	// === PROCESS SUMCHECK ROUNDS ===
	claim := claimAfterUniSkip

	// Round 0
	state, claim = processSumcheckRound(api, state, idx, claim,
		circuit.SumcheckR0C0, circuit.SumcheckR0C1, circuit.SumcheckR0C2,
		circuit.ExpectedChallenges[0], circuit.ExpectedClaimsAfterRound[0])
	idx += 4

	// Round 1
	state, claim = processSumcheckRound(api, state, idx, claim,
		circuit.SumcheckR1C0, circuit.SumcheckR1C1, circuit.SumcheckR1C2,
		circuit.ExpectedChallenges[1], circuit.ExpectedClaimsAfterRound[1])
	idx += 4

	// Round 2
	state, claim = processSumcheckRound(api, state, idx, claim,
		circuit.SumcheckR2C0, circuit.SumcheckR2C1, circuit.SumcheckR2C2,
		circuit.ExpectedChallenges[2], circuit.ExpectedClaimsAfterRound[2])
	idx += 4

	// Round 3
	state, claim = processSumcheckRound(api, state, idx, claim,
		circuit.SumcheckR3C0, circuit.SumcheckR3C1, circuit.SumcheckR3C2,
		circuit.ExpectedChallenges[3], circuit.ExpectedClaimsAfterRound[3])
	idx += 4

	// Round 4
	state, claim = processSumcheckRound(api, state, idx, claim,
		circuit.SumcheckR4C0, circuit.SumcheckR4C1, circuit.SumcheckR4C2,
		circuit.ExpectedChallenges[4], circuit.ExpectedClaimsAfterRound[4])
	idx += 4

	// Round 5
	state, claim = processSumcheckRound(api, state, idx, claim,
		circuit.SumcheckR5C0, circuit.SumcheckR5C1, circuit.SumcheckR5C2,
		circuit.ExpectedChallenges[5], circuit.ExpectedClaimsAfterRound[5])
	idx += 4

	// Round 6
	state, claim = processSumcheckRound(api, state, idx, claim,
		circuit.SumcheckR6C0, circuit.SumcheckR6C1, circuit.SumcheckR6C2,
		circuit.ExpectedChallenges[6], circuit.ExpectedClaimsAfterRound[6])
	idx += 4

	// Round 7
	state, claim = processSumcheckRound(api, state, idx, claim,
		circuit.SumcheckR7C0, circuit.SumcheckR7C1, circuit.SumcheckR7C2,
		circuit.ExpectedChallenges[7], circuit.ExpectedClaimsAfterRound[7])
	idx += 4

	// Round 8
	state, claim = processSumcheckRound(api, state, idx, claim,
		circuit.SumcheckR8C0, circuit.SumcheckR8C1, circuit.SumcheckR8C2,
		circuit.ExpectedChallenges[8], circuit.ExpectedClaimsAfterRound[8])
	idx += 4

	// Round 9
	state, claim = processSumcheckRound(api, state, idx, claim,
		circuit.SumcheckR9C0, circuit.SumcheckR9C1, circuit.SumcheckR9C2,
		circuit.ExpectedChallenges[9], circuit.ExpectedClaimsAfterRound[9])
	idx += 4

	// Round 10
	_, claim = processSumcheckRound(api, state, idx, claim,
		circuit.SumcheckR10C0, circuit.SumcheckR10C1, circuit.SumcheckR10C2,
		circuit.ExpectedChallenges[10], circuit.ExpectedClaimsAfterRound[10])

	// Final check (same as Stage1Circuit)
	api.AssertIsEqual(claim, circuit.ExpectedFinalClaim)

	return nil
}

// processSumcheckRound handles one sumcheck round with assertions
func processSumcheckRound(
	api frontend.API,
	state frontend.Variable,
	idx int,
	claim frontend.Variable,
	c0, c2, c3 frontend.Variable,
	expectedChallenge, expectedClaimAfter frontend.Variable,
) (frontend.Variable, frontend.Variable) {
	// Append round poly coefficients
	state = poseidon.Hash(api, state, idx, poseidon.ByteReverse(api, c0))
	state = poseidon.Hash(api, state, idx+1, poseidon.ByteReverse(api, c2))
	state = poseidon.Hash(api, state, idx+2, poseidon.ByteReverse(api, c3))

	// Derive challenge
	state = poseidon.Hash(api, state, idx+3, 0)
	challenge := poseidon.Truncate128Reverse(api, state)

	// CHECK: challenge matches expected
	api.AssertIsEqual(challenge, expectedChallenge)

	// Decompress polynomial: c1 = claim - 2*c0 - c2 - c3
	c1 := api.Sub(api.Sub(api.Sub(api.Sub(claim, c0), c0), c2), c3)

	// Evaluate polynomial at challenge: c0 + c1*r + c2*r^2 + c3*r^3
	r2 := api.Mul(challenge, challenge)
	r3 := api.Mul(r2, challenge)
	nextClaim := api.Add(
		api.Add(
			api.Add(c0, api.Mul(c1, challenge)),
			api.Mul(c2, r2)),
		api.Mul(c3, r3))

	// CHECK: claim after round matches expected
	api.AssertIsEqual(nextClaim, expectedClaimAfter)

	return state, nextClaim
}

// appendCommitments appends all 41Ã—12 commitment chunks to transcript
func appendCommitments(api frontend.API, state frontend.Variable, idx *int, circuit *DisaggregatedCircuit) frontend.Variable {
	// This is tedious but necessary - append each commitment chunk
	// Commitment 0
	state = poseidon.Hash(api, state, *idx, poseidon.ByteReverse(api, circuit.Commitment0Chunk0)); *idx++
	state = poseidon.Hash(api, state, *idx, poseidon.ByteReverse(api, circuit.Commitment0Chunk1)); *idx++
	state = poseidon.Hash(api, state, *idx, poseidon.ByteReverse(api, circuit.Commitment0Chunk2)); *idx++
	state = poseidon.Hash(api, state, *idx, poseidon.ByteReverse(api, circuit.Commitment0Chunk3)); *idx++
	state = poseidon.Hash(api, state, *idx, poseidon.ByteReverse(api, circuit.Commitment0Chunk4)); *idx++
	state = poseidon.Hash(api, state, *idx, poseidon.ByteReverse(api, circuit.Commitment0Chunk5)); *idx++
	state = poseidon.Hash(api, state, *idx, poseidon.ByteReverse(api, circuit.Commitment0Chunk6)); *idx++
	state = poseidon.Hash(api, state, *idx, poseidon.ByteReverse(api, circuit.Commitment0Chunk7)); *idx++
	state = poseidon.Hash(api, state, *idx, poseidon.ByteReverse(api, circuit.Commitment0Chunk8)); *idx++
	state = poseidon.Hash(api, state, *idx, poseidon.ByteReverse(api, circuit.Commitment0Chunk9)); *idx++
	state = poseidon.Hash(api, state, *idx, poseidon.ByteReverse(api, circuit.Commitment0Chunk10)); *idx++
	state = poseidon.Hash(api, state, *idx, poseidon.ByteReverse(api, circuit.Commitment0Chunk11)); *idx++
	// ... commitments 1-40 would follow the same pattern
	// For brevity, we'll generate this programmatically below
	return appendRemainingCommitments(api, state, idx, circuit)
}

// evaluateUniSkipPoly evaluates the uni_skip polynomial at r0
func evaluateUniSkipPoly(api frontend.API, circuit *DisaggregatedCircuit, r0 frontend.Variable) frontend.Variable {
	// Compute powers of r0
	r := make([]frontend.Variable, 28)
	r[0] = frontend.Variable(1)
	r[1] = r0
	for i := 2; i < 28; i++ {
		r[i] = api.Mul(r[i-1], r0)
	}

	// Sum coeff[i] * r0^i
	result := api.Add(
		api.Add(
			api.Add(
				api.Add(circuit.UniSkipCoeff0, api.Mul(circuit.UniSkipCoeff1, r[1])),
				api.Mul(circuit.UniSkipCoeff2, r[2])),
			api.Mul(circuit.UniSkipCoeff3, r[3])),
		api.Mul(circuit.UniSkipCoeff4, r[4]))

	result = api.Add(result, api.Mul(circuit.UniSkipCoeff5, r[5]))
	result = api.Add(result, api.Mul(circuit.UniSkipCoeff6, r[6]))
	result = api.Add(result, api.Mul(circuit.UniSkipCoeff7, r[7]))
	result = api.Add(result, api.Mul(circuit.UniSkipCoeff8, r[8]))
	result = api.Add(result, api.Mul(circuit.UniSkipCoeff9, r[9]))
	result = api.Add(result, api.Mul(circuit.UniSkipCoeff10, r[10]))
	result = api.Add(result, api.Mul(circuit.UniSkipCoeff11, r[11]))
	result = api.Add(result, api.Mul(circuit.UniSkipCoeff12, r[12]))
	result = api.Add(result, api.Mul(circuit.UniSkipCoeff13, r[13]))
	result = api.Add(result, api.Mul(circuit.UniSkipCoeff14, r[14]))
	result = api.Add(result, api.Mul(circuit.UniSkipCoeff15, r[15]))
	result = api.Add(result, api.Mul(circuit.UniSkipCoeff16, r[16]))
	result = api.Add(result, api.Mul(circuit.UniSkipCoeff17, r[17]))
	result = api.Add(result, api.Mul(circuit.UniSkipCoeff18, r[18]))
	result = api.Add(result, api.Mul(circuit.UniSkipCoeff19, r[19]))
	result = api.Add(result, api.Mul(circuit.UniSkipCoeff20, r[20]))
	result = api.Add(result, api.Mul(circuit.UniSkipCoeff21, r[21]))
	result = api.Add(result, api.Mul(circuit.UniSkipCoeff22, r[22]))
	result = api.Add(result, api.Mul(circuit.UniSkipCoeff23, r[23]))
	result = api.Add(result, api.Mul(circuit.UniSkipCoeff24, r[24]))
	result = api.Add(result, api.Mul(circuit.UniSkipCoeff25, r[25]))
	result = api.Add(result, api.Mul(circuit.UniSkipCoeff26, r[26]))
	result = api.Add(result, api.Mul(circuit.UniSkipCoeff27, r[27]))

	return result
}

